{% extends "template/private.twig" %}

{% block head_assets %}
    <link rel="stylesheet" href="{{ manifest['users/payment/process.css'] }}" />
{% endblock %}

{% block meta_data %}
    <meta id="offer-no-data" content="{{ numbers['offer'] }}">
    <meta id="order-no-data" content="{{ numbers['order'] }}">
    {% if env.analytics %}
        <script type="text/javascript" src="//www.googleadservices.com/pagead/conversion_async.js"></script>
    {% endif %}
{% endblock %}

{% block body_assets %}
    <script src="{{ manifest['users/payment/process.js'] }}"></script>
{% endblock %}

