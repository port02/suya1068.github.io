{
    "browser": true,
    "testRegex": "test\/.*\\.spec\\.js$",
    "testEnvironment": "node",
    "modulePaths": ["<rootDir>"],
    "moduleFileExtensions": ["js", "jsx", "json", "es6"],
    "transform": {
        "^.+\\.jsx?$": "babel-jest"
    },
    "moduleNameMapper": {
        "^.+\\.(css|scss)$": "identity-obj-proxy"
    }
}
