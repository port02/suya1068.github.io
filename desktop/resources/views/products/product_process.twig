{% extends "template/private.twig" %}

{% block head_assets %}
    <link rel="stylesheet" href="{{ manifest['products/product-process.css'] }}" />
{% endblock %}

{% block meta_data %}
    <meta id="product-data" content="{{ product['no'] }}">
    {% if env.analytics %}
        <script type="text/javascript" src="//www.googleadservices.com/pagead/conversion_async.js"></script>
    {% endif %}
{% endblock %}

{% block extra_body_assets_vendor %}
    {% if not env.bot %}
        <script src="https://code.jquery.com/jquery-2.2.4.js" integrity="sha256-iT6Q9iMJYuQiMWNd9lDyBUStIq/8PuOW33aOqmvFpqI=" crossorigin="anonymous"></script>
        <script type="text/javascript" src="https://service.iamport.kr/js/iamport.payment-1.1.2.js"></script>
    {% endif %}
{% endblock %}

{% block body_assets %}
    <script src="{{ manifest['products/product-process.js'] }}"></script>
{% endblock %}

